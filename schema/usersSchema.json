{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "username",
    "first_name",
    "last_name",
    "email"
  ],
  "properties": {
    "username": {
      "$id": "#/properties/username",
      "type": "string",
      "title": "The Username Schema",
      "default": "",
      "examples": [
        "ptam"
      ],
      "pattern": "^(.*)$"
    },
    "first_name": {
      "$id": "#/properties/first_name",
      "type": "string",
      "title": "The First_name Schema",
      "default": "",
      "examples": [
        "Parco"
      ],
      "pattern": "^(.*)$"
    },
    "last_name": {
      "$id": "#/properties/last_name",
      "type": "string",
      "title": "The Last_name Schema",
      "default": "",
      "examples": [
        "Tam"
      ],
      "pattern": "^(.*)$"
    },
    "email": {
      "$id": "#/properties/email",
      "type": "string",
      "title": "The Email Schema",
      "default": "",
      "examples": [
        "ptam@rithm.com"
      ],
      "pattern": "^(.*)$"
    },
    "photo_url": {
      "$id": "#/properties/photo_url",
      "type": "string",
      "title": "The Photo_url Schema",
      "default": "",
      "examples": [
        "https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRCZRdW_GBvY_lzXhuDxX--xTn7CmoBBIU3kpmMOj6gBTF2lLmp"
      ],
      "pattern": "^(.*)$"
    },
    "is_admin": {
      "$id": "#/properties/is_admin",
      "type": "boolean",
      "title": "The Is_admin Schema",
      "default": false,
      "examples": [
        false
      ]
    }
  }
}